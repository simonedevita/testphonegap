<?xml version='1.0' encoding='utf-8'?>
<widget id="it.simonedevita.firstapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>SDV</name>
    <description>
        The first app created with phonegap.
    </description>
    <author email="info@simonedevita.it" href="https://www.simonedevita.it">
        Simone De Vita
    </author>
    <content src="index.html" />
    <access origin="http://fuoriserie.altervista.org" />
    <access origin="https://cdnjs.cloudflare.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
